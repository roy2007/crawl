  <dependencies>
    <dependency>
      <groupId>org.springframework.boot</groupId>
      <artifactId>spring-boot-autoconfigure</artifactId>
    </dependency>

    <dependency>
      <groupId>org.projectlombok</groupId>
      <artifactId>lombok</artifactId>
    </dependency>
    <!--将被@ConfigurationProperties注解的类的属性注入到元数据 -->
    <dependency>
      <groupId>org.springframework.boot</groupId>
